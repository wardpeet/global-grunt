{
    "extends": "airbnb/legacy",
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "Promise": true
    },
    "rules": {
        "indent": [2, 4],
        "vars-on-top": 0,
        "no-use-before-define": 0,
        "func-names": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "global-require": 0
    }
}